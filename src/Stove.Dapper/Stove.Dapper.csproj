<Project Sdk="Microsoft.NET.Sdk">
  
  <Import Project="..\..\common.props"></Import>

  <PropertyGroup>
    <TargetFramework>net452</TargetFramework>
    <AssemblyName>Stove.Dapper</AssemblyName>
    <PackageId>Stove.Dapper</PackageId>
    <PackageTags>.net;framework;boilerplate;Dapper</PackageTags>
    <RootNamespace>Stove</RootNamespace>
    <Description>Dapper integration for Stove.</Description>
  </PropertyGroup>

  <ItemGroup>
    <None Include="bin\Release\net452\Stove.Dapper.pdb">
      <PackagePath>lib/net452/</PackagePath>
      <Pack>true</Pack>
    </None>
  </ItemGroup>

  <PropertyGroup>
    <DocumentationFile>bin\Release\net452\Stove.Dapper.xml</DocumentationFile>
  </PropertyGroup>
  
  <ItemGroup>
    <ProjectReference Include="..\Stove\Stove.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Dapper" Version="1.50.2" />
    <PackageReference Include="DapperExtensions" Version="1.6.1" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="System" />
    <Reference Include="Microsoft.CSharp" />
  </ItemGroup>

</Project>
